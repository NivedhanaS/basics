Sample 1:

Input:

n = 4

Expected output:

*

**

***

****
---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
#include <iostream>
using namespace std;
int main() {
    // Write C++ code her
    int n,i,j;
    cout << "n:";
    cin>>n;
    for (i=1;i<=n;i++){
        for(j=1;j<=i;j++){
            cout<< "*";
        }
        cout<<endl;
    }

    return 0;
}
